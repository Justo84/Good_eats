<h1> Create a New Restaurant Listing </h1>

<%= flash[:notice] %>

<%= form_for @restaurant do |f| %>
<div>
  <%= f.label :name %>
  <%= f.text_field :name %>
</div>

<div>
  <%= f.label :description %>
  <%= f.text_area :description %>
</div>

<div>
  <%= f.label :user_id %>
  <%= f.text_field :user_id %>
</div>

<div>
  <%= f.submit "Submit" %>
</div>

<% end %>
